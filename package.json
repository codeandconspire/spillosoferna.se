{
  "name": "spillosoferna.se",
  "version": "1.0.0",
  "description": "Educational platform Spillosoferna",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "deploy": "npx now --scope codeandconspire && npx now alias --scope codeandconspire",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/spillosoferna.se.git"
  },
  "browser": {
    "./lib/prismic-api": "./lib/prismic-api/browser"
  },
  "now": {
    "alias": "spillosoferna.conspire.dev",
    "env": {
      "NODE_ENV": "production",
      "CLOUDINARY_KEY": "@cloudinary-key",
      "CLOUDINARY_SECRET": "@cloudinary-secret",
      "PRISMIC_SECRET": "@prismic-spillosoferna-secret",
      "PRISMIC_TOKEN": "@prismic-spillosoferna-token",
      "SESSION_SECRET": "@session-spillosoferna-secret",
      "CLOUDFLARE_ZONE": "@cloudflare-spillosoferna-zone",
      "CLOUDFLARE_EMAIL": "@cloudflare-codeandconspire-email",
      "CLOUDFLARE_KEY": "@cloudflare-codeandconspire-key",
      "POLYFILL_FEATURES": "es2015,es2016,es2017"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "author": "code and conspire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeandconspire/spillosoferna.se/issues"
  },
  "homepage": "https://github.com/codeandconspire/spillosoferna.se#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "choo": "^7.0.0",
    "choo-devtools": "^3.0.1",
    "choo-lazy-view": "^1.0.2",
    "choo-meta": "^2.0.0",
    "choo-service-worker": "^3.0.0",
    "cloudinary": "^1.13.2",
    "dotenv": "^8.1.0",
    "hstream": "^1.2.0",
    "jalla": "^1.0.0-20",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-route": "^3.2.0",
    "koa-session": "^5.12.3",
    "nanoraf": "^3.1.0",
    "normalize.css": "^8.0.1",
    "postcss-focus-visible": "^4.0.0",
    "prismic-element": "^2.0.3",
    "prismic-javascript": "^2.1.1",
    "prismic-richtext": "^1.0.3"
  }
}
